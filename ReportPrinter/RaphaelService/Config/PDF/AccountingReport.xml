<?xml version="1.0" encoding="utf-8" ?>
<PdfTemplate Id="AccountingReport" PageSize="A4" Orientation="Portrait" SavePath=".\Result\PDF\" FileNameSuffix="AccountNumber">
	<ReportHeader Height="1*" Rows="1" Columns="4" Margin="0 20 0 20" Padding="0 10 0 10">

		
		<Text Margin="5 5 5 5" Padding="5 5 5 5" HorizontalAlignment="Left" VerticalAlignment="Center"
		      FontSize="9" FontFamily="Times New Roman" FontStyle="Regular" BrushColor="Black" BackgroundColor="" Opacity="1">
			<Type>Sql</Type>
			<Row>0</Row>
			<Column>0</Column>
			<RowSpan>1</RowSpan>
			<ColumnSpan>1</ColumnSpan>
			<SqlTemplateId>PrintPdfQuery</SqlTemplateId>
			<SqlId>GlAccount</SqlId>
			<SqlResColumn Id="Description" />
		</Text>

		<Image Margin="5 5 5 5" Padding="5 5 5 5" HorizontalAlignment="Left" VerticalAlignment="Center" BackgroundColor="">
			<Row>0</Row>
			<Column>1</Column>
			<RowSpan>1</RowSpan>
			<ColumnSpan>1</ColumnSpan>
			<SourceType>Local</SourceType>
			<ImageSource>.\Image\Logo.png</ImageSource>
		</Image>

		<!--
		<Annotation HorizontalAlignment="Center" VerticalAlignment="Center"
		            BrushColor="Yellow" BackgroundColor="Red" Opacity="0.5">
			<Type>Sql</Type>
			<Row>0</Row>
			<Column>2</Column>
			<RowSpan>1</RowSpan>
			<ColumnSpan>1</ColumnSpan>
			<Title>Description</Title>
			<Icon>Note</Icon>
			<SqlTemplateId>PrintPdfQuery</SqlTemplateId>
			<SqlId>GlAccount</SqlId>
			<SqlResColumn Id="Description" />
		</Annotation>

		<Text Margin="10 20 10 20" Padding="0 10 0 10" HorizontalAlignment="Left" VerticalAlignment="Center"
		      FontSize="9" FontFamily="Times New Roman" FontStyle="Italic" BrushColor="Red">
			<Type>Sql</Type>
			<Row>2</Row>
			<Column>2</Column>
			<SqlTemplateId>PrintPdfQuery</SqlTemplateId>
			<SqlId>GlAccount</SqlId>
			<SqlResColumn Id="Description" />
		</Text>
		-->
	</ReportHeader>
	<PageHeader Height="0.5*" Rows="1" Columns="4" Margin="0 20 0 20" Padding="0 10 0 10">
		
		<Image Margin="5 5 5 5" Padding="5 5 5 5" HorizontalAlignment="Left" VerticalAlignment="Center" BackgroundColor="">
			<Row>0</Row>
			<Column>0</Column>
			<RowSpan>1</RowSpan>
			<ColumnSpan>1</ColumnSpan>
			<SourceType>Local</SourceType>
			<ImageSource>.\Image\Logo.png</ImageSource>
		</Image>
		
	</PageHeader>

	<PageBody Height="8*" Margin="0 20 0 20" Padding="0 10 0 10">
		
		<Table Margin="5 5 5 5" Padding="5 5 5 5" HorizontalAlignment="Left" VerticalAlignment="Center" 
		       FontSize="9" FontFamily="Times New Roman" FontStyle="Regular" BrushColor="Black">
			<BoardThickness>1</BoardThickness>
			<LineSpace>1</LineSpace>
			<TitleHorizontalAlignment>Left</TitleHorizontalAlignment>
			<HideTitle>false</HideTitle>
			<SqlTemplateId>PrintPdfQuery</SqlTemplateId>
			<SqlId>VendorInfo</SqlId>
			<SqlResColumn Id="VendorID" Title="Vendor ID" Width="1*" />
			<SqlResColumn Id="VendorName" Title="Vendor Name" Width="2*" />
			<SqlResColumn Id="ShortName" Title="Short Name" Width="1*" />
			<SqlResColumn Id="GroupID" Title="Group ID" Width="1*" />
			<SqlResColumn Id="CurrencyCode" Title="Currency Code" Width="1*" />
<!--
			<Table Margin="5 5 5 5" Padding="5 5 5 5" HorizontalAlignment="Left" VerticalAlignment="Center"
			       FontSize="9" FontFamily="Times New Roman" FontStyle="Regular" BrushColor="Black">
				<Space>0.7</Space>
				<BoardThickness>1</BoardThickness>
				<LineSpace>0.5</LineSpace>
				<TitleHorizontalAlignment>Left</TitleHorizontalAlignment>
				<HideTitle>false</HideTitle>
				<SqlTemplateId>PrintPdfQuery</SqlTemplateId>
				<SqlId>VendorTransaction</SqlId>
				<SqlVariable>VendorID</SqlVariable>
				<SqlResColumn Id="TransactionID" Title="Transaction ID" Width="1.5*" />
				<SqlResColumn Id="GLAccount" Title="GL Account" Width="2*" />
				<SqlResColumn Id="Debit" Title="Debit" Width="1*" />
				<SqlResColumn Id="Credit" Title="Credit" Width="1*" />
				<SqlResColumn Id="InvoiceNo" Title="Invoice No" Width="1*" />
			</Table>
-->	
		</Table>
		<WaterMark Margin="5 5 5 5" Padding="5 5 5 5" FontSize="40" FontFamily="Times New Roman" BackgroundColor=""
				   FontStyle="Regular" BrushColor="Red" Opacity="0.5">
			<Type>Sql</Type>
			<SqlTemplateId>PrintPdfQuery</SqlTemplateId>
			<SqlId>GlAccount</SqlId>
			<SqlResColumn Id="AccountNumber" />
			<StartPage>1</StartPage>
			<EndPage>-2</EndPage>
			<Rotate>315</Rotate>
		</WaterMark>
		<PageNumber Margin="5 5 5 5" Padding="5 5 5 5" HorizontalAlignment="Right"
		            FontSize="9" FontFamily="Times New Roman" FontStyle="Regular" BrushColor="Black" BackgroundColor="" Opacity="1">
			<Location>Header</Location>
			<StartPage>1</StartPage>
			<EndPage>-1</EndPage>
		</PageNumber>

		<ReprintMark Margin="5 5 5 5" Padding="5 5 5 5" HorizontalAlignment="Right"
		             FontSize="12" FontFamily="Times New Roman" FontStyle="Regular" BrushColor="Red" BackgroundColor="" Opacity="1">
			<Text>REPRINT</Text>
			<BoardThickness>3</BoardThickness>
			<Location>Footer</Location>
		</ReprintMark>
	</PageBody>

	<PageFooter Height="0.5*" Rows="1" Columns="4" Margin="0 20 0 20" Padding="0 10 0 10">
		
		<Barcode Padding="0 20 0 20" FontSize="9" FontFamily="Calibri"
		         FontStyle="Regular" BrushColor="Navy" Opacity="1">
			<Row>0</Row>
			<Column>0</Column>
			<RowSpan>1</RowSpan>
			<ColumnSpan>1</ColumnSpan>
			<Format>CODE_128</Format>
			<ShowBarcodeText>true</ShowBarcodeText>
			<SqlTemplateId>PrintPdfQuery</SqlTemplateId>
			<SqlId>GlAccount</SqlId>
			<SqlResColumn Id="AccountNumber" />
		</Barcode>
	</PageFooter>
	<ReportFooter Height="1*" Rows="1" Columns="2" Margin="0 20 0 20" Padding="0 10 0 10">
		
		<Text Margin="5 5 5 5" Padding="5 5 5 5" HorizontalAlignment="Left" VerticalAlignment="Center"
		      FontSize="9" FontFamily="Times New Roman" FontStyle="Regular" BrushColor="Black" BackgroundColor="" Opacity="1">
			<Type>Text</Type>
			<Row>0</Row>
			<Column>0</Column>
			<RowSpan>1</RowSpan>
			<ColumnSpan>1</ColumnSpan>
			<Content>Hello World</Content>
		</Text>
	</ReportFooter>
</PdfTemplate>